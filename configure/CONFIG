unexport EPICS_MODULE_TAG
unexport EPICS_MODULE_SRC_PATH
unexport PROJECT
unexport LIBVERSION
unexport E3_ENV_NAME
unexport USR_DEPENDENCIES

EPICS_MODULE_NAME:=tsclib
# This is the module source tag
# *) individual tags
#    export EPICS_MODULE_TAG:=tags/1.1.1
# *) master branch 
#    export EPICS_MODULE_TAG:=master
# *) individual hash 
